        <script src="~/dist/site.min.js" asp-append-version="true"></script>
        <script src="~/dist/typescript.min.js" asp-append-version="true"></script>
    </environment>
    <script src="//maps.googleapis.com/maps/api/js?key=AIzaSyBk_OD93nBonF_lvpyu-9LHVkaIxjx151U&libraries=places"></script>
    <script>
        $(document).ready(function () {
            // inizializzo il framework
            CMS.init();

            /* owlCarousel2 */
            $('#owl-dashboard').owlCarousel({
                loop: false,
                nav: true,
                margin: 30,
                items: 1,
                navText: ['', '']
            });
        });

        // plugin scrollbar
        $(window).on("load", function () {
            $("#sidebar-dashboard").mCustomScrollbar();
        });
    </script>
}